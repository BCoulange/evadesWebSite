<h1>Listing affiche_templates</h1>

<table>


<% @affiche_templates.each do |affiche_template| %>
  <tr>
    <td><%= image_tag affiche_template.fond.url(:thumb) %></td>
    <td><b><%= affiche_template.nom %></b></td>
    <td><%= link_to 'Show', affiche_template %></td>
    <td><%= link_to 'Edit', edit_affiche_template_path(affiche_template) %></td>
    <td><%= link_to 'Destroy', affiche_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Affiche template', new_affiche_template_path %>
