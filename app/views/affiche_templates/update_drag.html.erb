<% content_for(:head) do %>
<%= javascript_include_tag 'jquery-ui'%>
  <style>
  #draggable { width: 150px; height: 150px; padding: 0.5em; }
  </style>
    <script type="text/javascript" charset="utf-8">
		$(function() {
		  $( "#draggable" ).draggable();
		});
    </script>

<% end %>

<div id="draggable" class="ui-widget-content">
  <p>Drag me around</p>
</div>