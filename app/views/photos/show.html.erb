<h1><%=@set.title%></h1>


<ul class="thumbnails">
	<% @photos.each do |p| %>
  		<li class="span5">
    				<a class="thumbnail" href=<%= "http://farm#{p.farm}.staticflickr.com/#{p.server}/#{p.id}_#{p.secret}.jpg" %>>
    	<div class="well"><%= image_tag "http://farm#{p.farm}.staticflickr.com/#{p.server}/#{p.id}_#{p.secret}.jpg", :class => "brand"%></div>
                		<p><%= p.title %></p></a>

	  		</li>
 		<% end %>

 		</ul>


<%= page_entries_info @photos %>
<%= will_paginate @photos %>
